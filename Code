n1 = int(input(':'))
n2 = int(input(':'))
n3 = int(input(':'))

if n1 + n2 > n3 and n1 + n3 > n2 and n2 + n3 > n1:
    if n1 == n2 == n3:
        print('Equilateral')
    elif n1 == n2 or n1 == n3 or n2 == n3:
        print('Isosecles')
    elif n1^2 + n2^2 == n3^2 or n1^2 + n3^2 == n2^2 or n3^2 + n2^2 == n1^2:
        print('Rectangle')
    else:
        print('Escalene')

else:
    print('Theres no triangle with that sizes.')


